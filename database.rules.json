{
  "rules": {
    "coupons": {
      ".read": "auth != null && auth.token.firebase.sign_in_provider === 'google.com'",
      ".write": "auth != null && auth.token.firebase.sign_in_provider === 'google.com'"
    },
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid && auth.token.firebase.sign_in_provider === 'google.com'",
        ".write": "auth != null && auth.uid === $uid && auth.token.firebase.sign_in_provider === 'google.com'",
        "coupons": {
          ".read": "auth != null && auth.uid === $uid && auth.token.firebase.sign_in_provider === 'google.com'",
          ".write": "auth != null && auth.uid === $uid && auth.token.firebase.sign_in_provider === 'google.com'"
        }
      }
    }
  }
}
